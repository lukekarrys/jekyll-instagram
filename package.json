{
  "name": "usi",
  "description": "A universal JS static site generator for Instagram built with React.",
  "version": "1.0.0",
  "author": "Luke Karrys",
  "bugs": {
    "url": "https://github.com/lukekarrys/universal-static-instagram/issues"
  },
  "dependencies": {
    "dateformat": "^2.0.0",
    "history": "^4.6.3",
    "lodash": "^4.17.4",
    "normalizr": "^3.2.3",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-ga": "^2.2.0",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "rebass": "^1.0.0-7",
    "redux": "^3.7.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "reflexbox": "^2.2.3",
    "xhr": "^2.4.0",
    "zero-fill": "^2.2.3"
  },
  "devDependencies": {
    "@lukekarrys/eslint-config": "^5.0.0",
    "async": "^2.5.0",
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-imports": "^1.4.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "colors": "^1.1.2",
    "common-tags": "^1.4.0",
    "cpr": "^2.2.0",
    "css-loader": "^0.28.4",
    "debug": "^2.6.8",
    "eslint": "^4.1.1",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-classes": "^0.1.1",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.1.0",
    "git-directory-deploy": "^1.5.1",
    "git-validate": "^2.2.2",
    "hjs-webpack": "^9.1.0",
    "html-tagged-literals": "^1.0.2",
    "http-server": "^0.10.0",
    "humps": "^2.0.1",
    "inquirer": "^3.1.1",
    "instagram-download": "^2.2.5",
    "instagram-node": "^0.5.8",
    "on-build-webpack": "^0.1.0",
    "postcss-loader": "^2.0.6",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "surge": "^0.19.0"
  },
  "engines": {
    "node": "^6.0.0",
    "npm": "^3.0.0"
  },
  "homepage": "https://github.com/lukekarrys/universal-static-instagram",
  "keywords": [
    "instagram",
    "isomorphic",
    "react",
    "react-router",
    "redux",
    "static site",
    "universal"
  ],
  "license": "MIT",
  "main": "index.js",
  "pre-commit": [
    "lint",
    "validate",
    "test"
  ],
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukekarrys/universal-static-instagram.git"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack --progress --bail",
    "build.local": "USI_LOGGER=true npm run build",
    "build.nojs": "USI_NOJS=true npm run build && rimraf public/*.js && rimraf public/json/",
    "deploy.gh": "git-directory-deploy --directory public",
    "deploy.surge": "surge public",
    "fetch.data": "DEBUG=instagram-download:* babel-node server/data/fetch.js --refresh",
    "lint": "eslint .",
    "make.config": "babel-node server/config/make.js",
    "start": "USI_LOGGER=true hjs-dev-server",
    "start.static": "http-server ./public -p 8080",
    "test": "rimraf test/public && USI_BUILD_DIR=test/public USI_CACHE_DIR=test/fixtures USI_CONFIG=test/config.json npm run build && rimraf test/public",
    "validate": "npm ls >/dev/null"
  }
}
