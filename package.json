{
  "name": "usi",
  "description": "A universal JS static site generator for Instagram built with React.",
  "version": "1.0.0",
  "author": "Luke Karrys",
  "bugs": {
    "url": "https://github.com/lukekarrys/universal-static-instagram/issues"
  },
  "dependencies": {
    "basscss": "^7.1.0",
    "history": "^1.17.0",
    "lodash": "^3.10.1",
    "moment": "^2.10.6",
    "normalizr": "^1.4.0",
    "react": "^0.14.3",
    "react-dom": "^0.14.6",
    "react-redux": "^4.0.5",
    "react-router": "^1.0.3",
    "rebass": "^0.1.5",
    "redux": "^3.0.5",
    "redux-logger": "^2.3.1",
    "redux-simple-router": "^1.0.2",
    "redux-thunk": "^1.0.2",
    "xhr": "^2.2.0",
    "zero-fill": "^2.2.1"
  },
  "devDependencies": {
    "@lukekarrys/eslint-config": "^1.0.37",
    "async": "^1.5.0",
    "autoprefixer": "^6.2.2",
    "babel": "^6.3.26",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "colors": "^1.1.2",
    "cpr": "^1.0.0",
    "css-loader": "^0.23.1",
    "cssnano": "^3.4.0",
    "debug": "^2.2.0",
    "eslint": "^1.10.3",
    "eslint-plugin-classes": "^0.1.1",
    "eslint-plugin-react": "^3.13.1",
    "git-directory-deploy": "^1.3.0",
    "hjs-webpack": "^6.0.0",
    "http-server": "^0.8.5",
    "humps": "^1.0.0",
    "inquirer": "^0.11.0",
    "instagram-download": "^1.1.4",
    "instagram-node": "^0.5.8",
    "minimist": "^1.2.0",
    "on-build-webpack": "^0.1.0",
    "postcss-loader": "^0.8.0",
    "precommit-hook": "^3.0.0",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-slider-monitor": "^1.0.0",
    "rimraf": "^2.5.0",
    "style-loader": "^0.13.0",
    "surge": "^0.17.4",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "homepage": "https://github.com/lukekarrys/universal-static-instagram",
  "keywords": [
    "instagram",
    "isomorphic",
    "react",
    "react-router",
    "static site",
    "universal"
  ],
  "license": "MIT",
  "main": "index.js",
  "pre-commit": [
    "lint",
    "validate",
    "test"
  ],
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukekarrys/universal-static-instagram.git"
  },
  "scripts": {
    "build": "npm run webpack.config && NODE_ENV=production webpack --progress",
    "build.local": "USI_LOGGER=true npm run build",
    "build.nojs": "USI_NOJS=true npm run build && rimraf public/*.js && rimraf public/json/",
    "deploy.gh": "git-directory-deploy --directory public",
    "deploy.surge": "surge public",
    "fetch.data": "DEBUG=instagram-download:* babel-node server/data/fetch.js",
    "lint": "eslint --ignore-path .gitignore --ext .js,.es6 .",
    "make.config": "babel-node server/config/make.js",
    "start": "npm run webpack.config && USI_LOGGER=true USI_DEVTOOLS=true webpack-dev-server --progress",
    "start.static": "http-server ./public -p 8080",
    "test": "USI_BUILD_DIR=test/public USI_CACHE_DIR=test/fixtures npm run build -- --user 100 && rimraf test/public",
    "validate": "rm -rf ./node_modules/redux-simple-router/node_modules/history && npm ls >/dev/null",
    "webpack.config": "babel webpack.config.es6 --out-file webpack.config.js"
  }
}
